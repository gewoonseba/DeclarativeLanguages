father(anton,bart).
father(anton,daan).
father(anton,elisa).
father(fabian,anton).

mother(celine,bart).
mother(celine,daan).
mother(celine,gerda).
mother(gerda,hendrik).

sibling(X,Y):-
        father(Z,X),
        father(Z,Y),
        mother(Q,X),
        mother(Q,Y),
        X \== Y.

ancestor(X,Y):-
        ancestor(X,Z),
        father(Z,Y).

ancestor(X,Y):- father(X,Y).
